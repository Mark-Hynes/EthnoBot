<body>
    <nav class="navbar fixed-top navbar-expand-md navbar-light bg-light" style="margin-top: 0;padding-top: 0;">
        @Html.ActionLink("EthnoBotanicals Ireland", "Index", "Home", new object { }, new { @class = "navbar-brand" })
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav nav-fill">
                <li class="nav-item">
                    @Html.ActionLink("Home", "Index", "Home", null, new { @class = "nav-link text-success" })
                </li>
                <li class="nav-item">
                    @Html.ActionLink("Products", "Index", "Search", null, new { @class = "nav-link text-info" })
                </li>
                @{
                    int cartCount = 0;
                    try { cartCount = (int)Session["CartItemCount"]; }
                    catch (Exception) { }
                }
                <li class="nav-item align-self-center form-inline">
                    @using (Html.BeginForm("SearchWString", "Search", new { @class = "" }))
                    {

                        <input class="form-control" type="text" name="searchString" placeholder="Find Products ">
                        <button type="submit" class="btn btn-success">Search</button>

                    }
                </li>

            </ul>
            @Html.Partial("_LoginPartial")
        </div>

    </nav>


    <div class="body-content">
        @RenderBody()

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
